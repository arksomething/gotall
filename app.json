{
  "expo": {
    "name": "GoTall",
    "slug": "gotall",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gotall",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.gotall.ios",
      "buildNumber": "4",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSUserNotificationUsageDescription": "We use notifications to remind you to stretch and stay active."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#C8FA43"
      },
      "edgeToEdgeEnabled": true,
      "package": "app.gotall.android",
      "versionCode": 3,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "BILLING"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-iap",
        {
          "androidStore": "play"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#C8FA43"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "modularHeaders": true,
            "googleServicesFile": "./GoogleService-Info.plist"
          },
          "android": {
            "googleServicesFile": "./google-services.json"
          }
        }
      ],
      "expo-sqlite",
      "expo-web-browser",
      "@react-native-firebase/app"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bb43a5e3-9cb1-4555-bab8-d7a323adf786"
      }
    },
    "runtimeVersion": "1.0.1",
    "updates": {
      "url": "https://u.expo.dev/bb43a5e3-9cb1-4555-bab8-d7a323adf786"
    }
  }
}
